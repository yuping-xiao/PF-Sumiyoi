<h1>Rooms#index</h1>
<p>Find me in app/views/rooms/index.html.erb</p>

<table class="table">
	<thead>
		<tr>
			<th>物件名</th>
			<th>住所</th>
			<th>居住年数</th>
		</tr>
	</thead>

	<tbody>

	<% @rooms.each do |f| %>

		<tr>
			<td><%= f.name %></td>
			<td><%= f.address %></td>
			<td><%= f.living_status %></td>
		</tr>

	<% end %>

	</tbody>

